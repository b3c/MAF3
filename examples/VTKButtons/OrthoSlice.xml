<compoundView>
	<splitter orientation="vertical" sizes="100, 100" separatorThikness="1" enableSeparator="1">
		<splitter orientation="orizontal" sizes="100, 100" separatorThikness="1" enableSeparator="1">
			<view classtype="mafPluginVTK::mafViewVTK" objectName="Perspective" cameraParallel="0">
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSurface" datatype="vtkPolyData"></visualpipe>
			</view>
			<view classtype="mafPluginVTK::mafViewVTK" objectName="ParallelLeft" cameraParallel="1">
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSliceSurface" datatype="vtkPolyData"></visualpipe>
			</view>
		</splitter>
		<splitter orientation="orizontal" sizes="100, 100" separatorThikness="1" enableSeparator="1">
			<view classtype="mafPluginVTK::mafViewVTK" objectName="ParallelTop" cameraParallel="1">
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSliceSurface" datatype="vtkPolyData"></visualpipe>
			</view>
			<view classtype="mafPluginVTK::mafViewVTK" objectName="ParallelRight" cameraParallel="1">
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSliceSurface" datatype="vtkPolyData"></visualpipe>
			</view>
		</splitter>
	</splitter>
