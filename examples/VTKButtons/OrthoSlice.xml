<compoundView>
	<splitter orientation="vertical" sizes="100, 100" separatorThikness="1" enableSeparator="1">
		<splitter orientation="orizontal" sizes="100, 100" separatorThikness="1" enableSeparator="1">
			<view classtype="mafPluginVTK::mafViewVTK" objectName="Perspective" cameraParallel="0">
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSurface" datatype="vtkPolyData"></visualpipe>
			</view>
			<view classtype="mafPluginVTK::mafViewVTK" objectName="Left" cameraParallel="1" cameraAxes="4">
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSliceSurface" datatype="vtkPolyData" normalY="1" normalZ="0"></visualpipe>
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSliceVolume" datatype="vtkStructuredPoints" sliceAxes="1"></visualpipe>
			</view>
		</splitter>
		<splitter orientation="orizontal" sizes="100, 100" separatorThikness="1" enableSeparator="1">
			<view classtype="mafPluginVTK::mafViewVTK" objectName="Top" cameraParallel="1" cameraAxes="5">
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSliceSurface" datatype="vtkPolyData"></visualpipe>
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSliceVolume" datatype="vtkStructuredPoints" sliceAxes="2"></visualpipe>
			</view>
			<view classtype="mafPluginVTK::mafViewVTK" objectName="Front" cameraParallel="1" cameraAxes="3">
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSliceSurface" datatype="vtkPolyData" normalX="1" normalZ="0"></visualpipe>
				<visualpipe classtype="mafPluginVTK::mafPipeVisualVTKSliceVolume" datatype="vtkStructuredPoints" sliceAxes="0"></visualpipe>
			</view>
		</splitter>
	</splitter>
</compoundView>